{
	"name": "stash",
	"displayName": "stash",
	"description": "open code in stash",
	"publisher": "stash-integration",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openInStash",
				"title": "Open In Stash"
			},
			{
				"command": "extension.copyStashLink",
				"title": "Copy Stash Link"
			},
			{
				"command": "extension.getPullRequestComments",
				"title": "Get Comments From Stash"
			}
		],
		"menus": {
			"editor/context": [
				{
					"group": "bookmarks",
					"command": "extension.openInStash"
				},
				{
					"group": "bookmarks",
					"command": "extension.copyStashLink"
				}
			]
		},
		"configuration": {
			"title": "Stash",
			"properties": {
				"stash.apiKey": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "Stash apikey with read access."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2"
	}
}
